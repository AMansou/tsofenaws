echo you can only use the commands : ls/pwd/free/exit
echo 
while :
do
  read command
  if [[ $command = 'ls' ]]
  then
    ls
  elif [[ $command = 'pwd' ]]
  then
    pwd
  elif [[ $command = 'free' ]]
  then
    free
  elif [[ $command = 'exit' ]]
  then
    exit
  else
    echo try again
  fi


done
